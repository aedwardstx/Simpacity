<h3>SNMP Settings</h3>

<table id='dt_snmp' width=100%>
  <thead>
    <tr>
      <th>Name</th>
      <th>Community String</th>
      <th>Default</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @snmps.each do |snmp| %>
      <tr>
        <td><%= snmp.name %></td>
        <td><%= snmp.community_string %></td>
        <td><%= snmp.default_community %></td>
        <td>
          <%= link_to 'Edit', edit_snmp_path(snmp), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', snmp, method: :delete, data: { confirm: 'Are you sure?'}, :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snmp', new_snmp_path, :class => 'btn btn-xs' %>

<% content_for :sidebar do %>
  <h4>SNMP Settings</h4>
  </br>
  Configure SNMP communities here.
<% end %>

